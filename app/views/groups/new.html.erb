<h1>Make a new group</h1>
<%= form_for @group do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>

  <%= f.submit %>
<% end %>
<canvas data-processing-sources=<%= asset_path 'wave.pde' %>></canvas>
