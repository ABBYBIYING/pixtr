<%= form_for @group do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>

  <div>
    <%= f.label :group_image %>
    <%= f.text_area :group_image %>
  </div>

  <%= f.submit %>
<% end %>
