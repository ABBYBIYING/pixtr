<canvas data-processing-sources=<%= asset_path 'hello-web.pde' %>></canvas>
<h3>Here are images you created.</h3>
<ul>
<% current_user.images.each do |image| %>
  <li>
    <%= link_to (image_tag image.url, width:"300"), gallery_image_path(image.gallery, image) %>
  </li>
  <% end %>
</ul>

<% if current_user.images %>
<h3>Here are images you liked.</h3>

<ul>
<% current_user.liked_images.each do |image| %>
  <li>
    <%= link_to (image_tag image.url, width:"300"), gallery_image_path(image.gallery, image) %>
  </li>
<% end %>
</ul>
<% else %>
<p>You do not have any images yet.</p>
<% end %>

<h3>Here are the group you are a member of <h3>
<ul>
<% current_user.groups.each do |group| %>
  <li>
    <%= link_to group.name, group_path(group) %>
  </li>
    <% end %>
</ul>
</h3>
