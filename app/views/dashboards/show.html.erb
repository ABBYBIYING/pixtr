<canvas data-processing-sources=<%= asset_path 'hello-web.pde' %>></canvas>

<%= render(@user.images) || "<p>No images yet.</p>".html_safe %>

<%= render(@user.groups) || "<p>No groups yet.</p>".html_safe %>

<%= render(@user.comments) || "<p>No comments yet.</p>".html_safe %>

<% if current_user.liked_images.present? %>
  <h3> here are the images you like:</h3>
  <ul>
  <% current_user.liked_images.each do |image| %>
    <li>
      <%= link_to (image_tag image.url, width:"200"), gallery_image_path(image.gallery, image) %>
    </li>
  <% end %>
  </ul>
<% else %>
  <p>No liked images yet.</p>
<% end %>
