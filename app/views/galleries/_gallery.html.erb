<%= link_to gallery do%>
  <h2>Gallery: <%= gallery.name %>, description: <%= gallery.description %></h2>
  <p><%= image_tag gallery.cover_image %></p>
<% end %>

<% gallery.images.each do |image| %>
  <div>
    <%= link_to (image_tag image.url), gallery_image_path(gallery, image) %>
  </div>
<% end %>
<hr>
